pragma solidity 0.8.27;

import {ERC721} from "solady/tokens/ERC721.sol";

contract LeaderBoard is ERC721 {
    address immutable private _OWNER;
    
    uint256 public totalSupply;
    
    constructor() {
        // @notice the owner of this contract is the verifier contract
        _OWNER = msg.sender;
    }
    function name() public view override returns (string memory) {
        return "Sokoban LeaderBoard";
    }

    function symbol() public view override returns (string memory) {
        return "SOK";
    }
    function tokenURI(uint256) public view override returns (string memory) {
        return "data:image/svg+xml;base64,";
    }

    function mint(address leader) external {
        require(msg.sender == _OWNER, "!OWNER");
        _mint(leader, totalSupply++);
    }
}
